{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(js|css|png|jpg|svg|woff|woff2|ttf|map))",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist"
  }
  